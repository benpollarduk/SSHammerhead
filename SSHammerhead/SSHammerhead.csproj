<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
  </PropertyGroup>

  <PropertyGroup>
    <LangVersion>12.0</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Images\space.jpg" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Images\space.jpg">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="NetAF" Version="2.10.0" />
    <PackageReference Include="NetAF.Imaging" Version="1.0.3" />
  </ItemGroup>

</Project>
