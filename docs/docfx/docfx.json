{
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "templates/singulinkfx"
    ],
    "globalMetadata": {
      "_appName": "SSHammerhead",
      "_appTitle": "SSHammerhead",
      "_appLogoPath": "images/logo.png",
      "_appFaviconPath": "images/logo.png",
      "_enableSearch": false,
      "pdf": false,
      "_disableContribution": true,
      "_disableNextArticle": true
    }
  }
}
